	ZoomOut:
		TNT1 A 0
		{
			invoker.iron_sight = false;
			A_ZoomFactor(1.0);
			A_SetCrosshair(0);
		}
		4AFZ EDCBA 1;
	Goto ready_loop;

	Reload:
		TNT1 A 0
		{
			A_ZoomFactor(1.0);
		}

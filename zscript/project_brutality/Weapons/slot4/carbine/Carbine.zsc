	TNT1 A 0 A_JumpIfInventory("XRifleAmmo",1,1);
	Goto Reload;
	4A2F C 1 {
		A_ZoomFactor(1.5);
	}
	Goto ready_loop;
	
	
	NoAmmo:
		4AFG A 1 {
			A_ZoomFactor(1.0);
			A_PlaySound("weapons/empty", 0);


	}
	Goto ready_loop;
	

		
	AlreadyUnloaded:

		Goto ready_loop;
		
	Unload:

		TNT1 A 0 {
			A_ZoomFactor(1.0);



		}
		//TNT1 A 0 A_JumpIfInventory("CarbineHasUnloaded", 1, "AlreadyUnloaded");
		TNT1 A 0 A_JumpIfInventory("XRifleAmmo",1,1);
	Goto NoAmmo;

		TNT1 A 0 A_SetCrosshair(5);
		TNT1 A 0 A_PlaySound("Ironsights", 0);
		4AFR ABCDE 1;
		4AFR FGHIJ 1 A_SetRoll(roll+0.8);
		TNT1 A 0 A_PlaySound("weapons/carbine/magout", 0);
		4AFR KLMNO 1 A_SetRoll(roll-0.8);
		4AFR PPQRST 1 A_SetRoll(roll-0.9);
		4AFA AAA 1 A_SetRoll(roll+1.8);
	RemoveBullets:
		TNT1 A 0 A_JumpIfInventory("XRifleAmmo",1,1);
		Goto FinishUnload;
		TNT1 A 0 {
			A_Takeinventory("XRifleAmmo",1);

			A_Giveinventory("pb_clip",1);
	}
	Goto RemoveBullets;
	
	FInishUnload:
		4AFR CBA 1;


		Goto ready_loop;
		
		
		
	FlashKicking:
	FlashAirKicking:
		4A1K ABCDEFGHIJKLMNNN 1;
		Stop;
	FlashSlideKicking:
		4A1K ABCDEFGPQRSTUTSRQGHIJKLMNNN 1;
		Stop;
	FlashSlideKickingStop:
		4A1K HIJKLM 1;
		Stop;
	FlashPunching:
		4AFG A 1;
		4AFG B 1;
		4AFG BB 1;
		4AFG C 2;
		4AFG D 3;
		4AFG C 2;
		4AFG BB 1;
		4AFG B 1;
		4AFG A 1;
		4AFG A 1;
		Stop;
	}
}


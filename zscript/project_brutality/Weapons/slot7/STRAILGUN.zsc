		R5IR DEFGH 2;
		TNT1 A 0 A_Playsound("PLREADY",4, 3);
		R5IR JKLMN 1;
		TNT1 A 0 A_PlaySound("weapons/rail_up",3, 6);
		R5IR OPQR 1;
		Goto InsertBullets;
		////////////////////////////////////////Unloading	
		
	AlreadyUnloaded:
		TNT1 A 0 A_Takeinventory("Unloading",1);
		Goto Ready+6;
		
	Unload:
		RAIR A 1 A_WeaponReady;
		TNT1 A 0 A_JumpIfInventory("HasUnloadedRG", 1, "AlreadyUnloaded");

		TNT1 A 0 a_fireprojectile("PlasmaCaseSpawn",-30,0,-4,-4);
		TNT1 A 0;
		R4IR QRST 2;
		TNT1 A 0 A_Playsound("Weapons/CellKickIn",6, 5);
		R4IR UVWXYZ 1;
		R5IR AB 2;
		TNT1 A 0 A_Playsound("weapons/railpunch",5, 4);

		RA1F A 1 BRIGHT A_PlaySound("Weapons/StachanovAddFire",1);
		TNT1 A 0 A_PlaySound("Weapons/StachanovAddFire",3);

		TNT1 A 0 a_fireprojectile("GunFireSmoke",0,0,0,5);

		R4IR ABCDEF 1;
		TNT1 A 0 A_Playsound("PLSM2RL",7, 1);
		R4IR GHIJ 2;
		TNT1 A 0 A_JumpIfInventory("HasUnloadedRG", 1, 8);
		TNT1 A 0 A_Playsound("Weapons/CellEject",2,2);

CLASS SpiderPart1 : ACTOR { 
Default
{
    Radius 1;
    Height 8;
    Speed 12;
    Scale 1.3;
	Mass 1;
    +NOBLOCKMAP
	+MISSILE
    +NOTELEPORT
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
    +RIPPER
    +BLOODLESSIMPACT 
	damagetype "Blood";
    SeeSound "misc/xdeath4";
    DeathSound "misc/xdeath1";
	Decal "BloodSuper";
}

States
{
    Spawn:
	    TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash");
        ID22 ABCD 2 A_spawnprojectile("Brutal_FlyingBloodTrails",0,0,180,2);
	Loop;

	Death:
        ID22 E 1 A_SpawnItem("pb_Blood_Spot",0,0,0,1);
        ID22 E -1;
	Stop;
        TNT1 A 1;
     Splash:
		TNT1 A 0;
	Stop;
}

}



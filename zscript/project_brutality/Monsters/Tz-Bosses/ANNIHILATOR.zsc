	BloodType "Brutal_Blood";
	Obituary "%o was destroyed by an Annihilator";
	Species "CyberDemon";
	SeeSound "monster/annsit";
	PainSound "monster/annpain";
	DeathSound "monster/anndth";
	ActiveSound "monster/annidle";
}
States
{
	Death.KillMe:
	Death.Taunt:
		TNT1 A 0 {self.bSOLID = false;}
		TNT1 A 0 A_SpawnItem("The_Cyber_demon");
	Stop;
		
	ReplaceToggle:
		TNT1 A 0 {self.bTHRUACTORS = true;}
		TNT1 A 0 A_ChangeCountFlags(false);
		TNT1 A 0 A_SpawnItemEx("The_Cyber_demon",0,0,0,0,0,0,0,288);
	Stop;

	Spawn:
		//TNT1 A 0 ACS_ExecuteAlways(796, 0, 0, 0, 0);//Check if Vanilla Mode is activated
	Stand:
		TNT1 A 0;
		4NNI D 10 A_Look();
	loop;
		
	See: 
		TNT1 A 0;
		TNT1 A 0 A_PlaySound("monster/anhoof",4);
	RAGE:
		TNT1 A 0;
		TNT1 A 0 A_CheckSight("See2");
		EXPL A 0 Radius_Quake(6, 100, 0, 22, 0);
		ANBT AB 2;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT EDC 2;
		ANBT BA 2;
		TNT1 A 0 HealThing(4000);
		TNT1 A 0 A_Jump(96, "Missile3");
	Goto See2;
		
	See2:
		4NNI A 3 A_Playsound("monster/anhoof", 3);
		4NNI A 3;
		4NNI AA 2 A_Chase();
		4NNI BB 2 A_Chase();
		4NNI BBC 2 A_Chase();
		4NNI A 0 A_Playsound("monster/anhoof",2);
		4NNI C 2 A_Chase();
		4NNI CCC 2;
		4NNI D 2 A_Chase();
		//TNT1 A 0 A_SpawnItem("CybersHitBox", 0, 100,0);
		4NNI D 2 A_Chase();
		4NNI DD 2 A_Chase();
	Loop;
		
	Missile:
		TNT1 A 0 A_Jump(96, "Missile2");
		TNT1 A 0 A_Jump(64, "Missile3");
		TNT1 A 0 A_JumpIfCloser(16, "Missile4");
		4NNI ABCDE 1 A_FaceTarget();
		4NNI E 4 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,-7,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,7,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI E 3 A_FaceTarget();
		4NNI EDCBA 1 A_FaceTarget();
	Goto See2;
		
	Missile2:
		4NNI ABCDE 1 A_FaceTarget();
		4NNI E 8 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,-7,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,7,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI E 6 A_FaceTarget();
		4NNI EDCBA 1 A_FaceTarget();
	Goto See2;
		
	Missile3:
		4NNI ABCDE 1 A_FaceTarget();
		4NNI E 10 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI E 8 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,-7,0);
		4NNI E 6 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,7,0);
		4NNI E 4 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		4NNI E 3 A_FaceTarget();
		4NNI F 3 BRIGHT a_spawnprojectile("CyberBalls",60,25,0,0);
		4NNI EDCBA 1 A_FaceTarget();
	Goto See2;
		
	Missile4:
		4NNI ABCDE 1 A_FaceTarget();
		4NNI E 6 A_FaceTarget();
		TNT1 AA 0 a_spawnprojectile("CyberBalls",60,25,-7,0);
		4NNI F 3 BRIGHT;
		4NNI E 6 A_FaceTarget();
		TNT1 AA 0 a_spawnprojectile("CyberBalls",60,-25,-7,0);
		4NNI F 3 BRIGHT;
		4NNI E 6 A_FaceTarget();
		TNT1 AA 0 a_spawnprojectile("CyberBalls",60,25,-7,0);
		4NNI F 3 BRIGHT;
		TNT1 A 0 A_Jump(32, "Missile3");
		4NNI EDCBA 1 A_FaceTarget();
	Goto See2;
		
	Stomp:
		4NNI A 5 {A_FaceTarget(); self.bNORADIUSDMG = true;}
		ANMA A 7 A_FaceTarget();
		TNT1 A 0 a_spawnprojectile("CyberStomp",10,0,0,0);
		ANMA B 7;
		ANMA B 7 {self.bNORADIUSDMG = false;}
	Goto See2;
		
	Pain: 
		4NNI G 10 A_Pain();
	Goto See2;
		
	Death:
		4NNI HHHHH 2 ;//a_spawnprojectile("ExplosionSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		//TNT1 A 0 ACS_NamedExecuteAlways("CyberDemonDoorFix", 0, 0, 0, 0)//Fix for map 26 of starter pack
		EXPL A 0 Radius_Quake(6, 100, 0, 22, 0);
		TNT1 AAAA 0 ;//a_spawnprojectile("ExplosionSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAAAAA 0 ;//a_spawnprojectile("ExplosionSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAAA 0 a_spawnprojectile("UltraGoreSPawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 A 0 a_spawnprojectile("XDeathCyberdemonHead", 120, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAAAAA 0 a_spawnprojectile("UltraGoreSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 A 0 a_spawnprojectile("CyberXDeath", 0, 0, random(0, 360), 2, random(0, 160));
		4NNI I 5 A_Scream();
		4NNI JKL 1;
		4NNI M 1 A_NoBlocking();
		4NNI NOP 1;
		4NNI P 120;
		TNT1 A 0 A_BossDeath();
		4NNI P -1 A_BossDeath();
	Stop;
		
	Death.Decaptate:
	Death.Head:
	Death.MinorHead:
		TNT1 A 0 a_spawnprojectile("XDeathCyberdemonHead", 120, 0, random(0, 360), 2, random(0, 180));
		TNT1 A 0 A_SpawnItem("ShoqueAzul");
		TNT1 A 0 A_PlaySound("caco/shotx");
		TNT1 AAAAAAAAA 0 a_spawnprojectile("XDeath1", 120, 0, random(0, 360), 2, random(0, 180));
		//TNT1 A 0 ACS_NamedExecuteAlways("CyberDemonDoorFix", 0, 0, 0, 0)//Fix for map 26 of starter pack
		CYBB AAAAA 8 A_SpawnItem("MuchBlood", 0, 120);
		CYBB ABCABCB 8 A_SpawnItem("MuchBlood", 0, 120);
		EXPL A 0 Radius_Quake(6, 100, 0, 22, 0);
		TNT1 AAAA 0 ;//a_spawnprojectile("ExplosionSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAAAAA 0 ;//a_spawnprojectile("ExplosionSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAAA 0 a_spawnprojectile("UltraGoreSPawner", 90, 0, random(0, 360), 2, random(0, 180));
		
		TNT1 AAAAAA 0 a_spawnprojectile("UltraGoreSpawner", 90, 0, random(0, 360), 2, random(0, 180));
		TNT1 A 0 a_spawnprojectile("CyberXDeath", 0, 0, random(0, 360), 2, random(0, 160));
		CYBR I 0 A_Fall();
		CYBR JKL 0;
		CYBR M 0 A_NoBlocking();
		CYBR NOP 0;
		CYBR P 120;
		TNT1 A 0 A_BossDeath();
		CYBR P -1 A_BossDeath();
	Stop;
		
	Crush:
		TNT1 A 0 A_PlaySound("misc/xdeath4");
		CRS3 A 5;
		CRS3 A -1;
	Stop;
		
	Pain.Taunt:
		TNT1 A 0;
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, "Missile4");
		SPOS B 0
		{
			if(self.rage == true){self.setstatelabel("SpecialAttack");}
			A_Pain(); self.bMISSILEMORE = true; self.bMISSILEEVENMORE = true;
		}
		TNT1 A 0 A_PlaySound("monster/annsit");
		ANBT AB 2;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT CDE 3;
		ANBT DCD 3;
		ANBT EDC 2;
		ANBT BA 2;
		TNT1 A 0 A_GiveInventory("Enraged", 1);
	Goto Missile;
		
	SpecialAttack:
		CYBR E 6 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		TNT1 A 0 A_JumpIfCloser(180, "Stomp");
		CYBR E 4 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,-7,0);
		TNT1 A 0 A_JumpIfCloser(180, "Stomp");
		CYBR E 4 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,7,0);
		TNT1 A 0 A_JumpIfCloser(180, "Stomp");
		CYBR E 4 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		TNT1 A 0 A_JumpIfCloser(180, "Stomp");
		CYBR E 4 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
		CYBR E 4 A_FaceTarget();
		CYBR F 4 BRIGHT a_spawnprojectile("CyberBalls",60,-25,0,0);
	Goto See;
}

}
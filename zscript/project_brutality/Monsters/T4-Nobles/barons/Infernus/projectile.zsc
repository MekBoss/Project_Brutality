CLASS BruiserSpike : ACTOR { 
	Default {
	Scale 0.8;
	Radius 6;
	Height 8;
	Projectile;
	Renderstyle "Translucent";
	DamageType "ExplosiveImpact";
	+FLOORHUGGER;
	+NOCLIP;
}
	States
	{
	Spawn:
		SPIK A 0;
		TNT1 A 0 a_spawnprojectile("BigRicoChet");
		TNT1 A 0 A_SpawnItem("BigRicoChet", 0, -30);
		TNT1 A 0 A_SpawnItemEx("crutch",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		SPIK A 1 A_PlaySound("superbaron/spike");
		EXPL A 0 Radius_Quake(2, 24, 0, 15, 0);
		TNT1 AA 0 a_spawnprojectile("ExplosionParticle", 0, 0, random(0, 360), 2, random(0, 180));
		TNT1 AAA 0 a_spawnprojectile("ExplosionParticle", 0, 0, random(0, 360), 2, random(0, 360));
		TNT1 AAA 0 a_spawnprojectile("ExplosionParticle", 0, 0, random(0, 360), 2, random(0, 360));
		TNT1 AAA 0 a_spawnprojectile("ExplosionFlames", 0, 0, random(0, 360), 2, random(0, 360));
		SPIK BC 1;
		SPIK D 1 Radius_Quake(2,5,0,2,0);
		SPIK EF 1;
		SPIK G 0 A_Explode(45,150,0);
		SPIK G 1;
		SPIK GFEDCB 2;
		SPIK A 50;
		SPIK AAAAAAAAAAAAAAAAAAAA 1 A_FadeOut(0.05);
	Stop;

	Death:
		TNT1 A 1;
	Stop;
}
}
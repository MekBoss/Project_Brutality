{
	Spawn:
		TNT1 A 1 spawn("BruiserSpike", pos:self.pos);
		TNT1 A 5;
		TNT1 A 5 A_SpawnItemEx("WalkFire",0,0,0,1,0,0,0,128);
	Loop;
	Death:  
		TNT1 A 6;
	Stop;
}
}


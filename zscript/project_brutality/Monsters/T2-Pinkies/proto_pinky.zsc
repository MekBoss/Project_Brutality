CLASS BurningDemon: actor { 
States
{
	Spawn:
		//just demon
		//SBRG
		//demon 2
		//CRB7
		//Void
		//CRB8
		//mean 
		//CRB9
		
		TNT1 A 0
		{
			A_PlaySound("BURNZOM");
			switch(getclassname())
			{
				case 'pb_Bull_Demon' : sprite = getspriteindex("SBRG"); break;
				case 'pb_Mech_Demon' : sprite = getspriteindex("CRB7"); break;
				case 'pb_void_spectre' : sprite = getspriteindex("CRB8"); break;
				case 'pb_Demon_X' : sprite = getspriteindex("CRB9"); break;
				
				default: break;
			}
		}
	living_torch:
		"####" AAABBBCCCDDD 1
		{
			A_Wander();
			A_Recoil(-1);
			a_spawnprojectile("BurnParticles", 20, 0, random(0, 360), 2, random(50, 130));
			a_spawnprojectile("ExplosionParticle", 40, 0, random(0, 360), 2, random(50, 130));
		}
		tnt1 a 0 a_jumpif(random(0, 10) > 3, "living_torch");
		
	Death:
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_Scream;
		TNT1 A 0 A_playsound("demon/pain");
		P1N7 AAABBBzCCCDDDzCCCDDDzCCCDDDzCCCDDDzCCCDDD 3
		{
			a_spawnprojectile("FlameTrails", 30, 0, random(0, 360), 2, random(50, 130));
			if(frame > 20)
			{
				A_playsound("demon/pain");
				tics = 0;
			}
		}
	goto burned_demon;
		
	XDeath:
	Death.SSG:
	Death.Explosive:
		TNT1 A 0 A_NOBlocking;
		TNT1 AAAAAA 0 bright a_spawnprojectile("SuperGoreSpawner", 5, 0, random(0, 360), 2, random(30, 180));
		TNT1 AA 0 a_spawnprojectile("XDeathBurnedDemonArm", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 AA 0 a_spawnprojectile("XDeathBurnedDemonLeg", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 AAAAAA 0 a_spawnprojectile("XDeathBurnedMeat", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 A 0 a_spawnprojectile("MuchMeatDeath", 35, 0, random(0, 360), 2, random(0, 160));
		TNT1 AAAAAA 0 a_spawnprojectile("BurningFlyingMeatPiece", 64, 0, random(0, 360), 2, random(0, 160));
		TNT1 A 0 A_SpawnItem("CrueltyBonus5Health");
	Stop;

		//demon 2
		//CRB7
		
		CRB7 MNOP 3
		{
			A_Wander();
			a_spawnprojectile("BurnParticles", 25, 0, random(0, 360), 2, random(50, 130));
		}
		tnt1 a 0 a_jumpif(random(0, 10) < 3, "Death");
	Loop;
		
	Death:
		TNT1 A 0 {self.bSHOOTABLE = false;}
		TNT1 A 0 A_Jump(96, "Death2");
		CRB7 Q 2;
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_playsound("demon/death");
		CRB7 QQQQRRRR 2 a_spawnprojectile("BurnParticles", 35, 0, random(0, 360), 2, random(50, 130));
		goto burned_demon;
		
	Death2:
		TNT1 A 0 {self.bSHOOTABLE = false;}
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_playsound("demon/pain");
	dd2_loop:
		P1N7 AAABBBCCCDDD 3 a_spawnprojectile("FlamethrowerFireParticles", 20, 0, random(0, 360), 2, random(50, 130));
		TNT1 A 0 A_playsound("demon/pain");
		tnt1 a 0 a_jumpif(random(0, 10) < 7, "dd2_loop");

		TNT1 A 0 A_playsound("demon/death",5);
		goto burned_demon;
		
	XDeath:
	Death.SSG:
	Death.Explosive:
		TNT1 A 0 A_NOBlocking;
		TNT1 AAAAAA 0 bright a_spawnprojectile("SuperGoreSpawner", 5, 0, random(0, 360), 2, random(30, 180));
		TNT1 AA 0 a_spawnprojectile("XDeathBurnedDemonArm", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 AA 0 a_spawnprojectile("XDeathBurnedDemonLeg", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 AAAAAA 0 a_spawnprojectile("XDeathBurnedMeat", 32, 0, random(0, 360), 2, random(0, 160));
		TNT1 A 0 a_spawnprojectile("MuchMeatDeath", 35, 0, random(0, 360), 2, random(0, 160));
		TNT1 AAAAAA 0 a_spawnprojectile("BurningFlyingMeatPiece", 64, 0, random(0, 360), 2, random(0, 160));
		TNT1 A 0 A_SpawnItem("CrueltyBonus5Health");
		Stop;





		//mean 
		//CRB9
		CRB9 MNOP 3
		{
			A_Wander();
			a_spawnprojectile("BurnParticles", random(24,26), random(-4,4), random(0, 360), 2, random(50, 130));
		}
		TNT1 A 0 A_playsound("demon/pain");
	Death:
		CRB9 Q  2
		{
			self.bSHOOTABLE = false;
			A_NoBlocking();
			A_playsound("demon/death");
		}
		CRB9 QQQQRRRR 2 a_spawnprojectile("BurnParticles", 26, 0, random(0, 360), 2, random(50, 130));
	Stop;


	//Void
	//CRB8
		CRB8 MNOP 3
		{
			A_Wander();
			a_spawnprojectile("BurnParticles", random(24,26), random(-4,4), random(0, 360), 2, random(50, 130));
		}
		TNT1 A 0 A_playsound("demon/pain");
		tnt1 q 0 a_jump(33, 1);
	Loop;
	
		TNT1 A 0 {self.bSHOOTABLE = false;}
		CRB8 Q 2
		{
			A_NoBlocking();
			A_Scream();
			A_playsound("demon/death");
		}
		CRB8 QQQQRRRR 2 a_spawnprojectile("BurnParticles", 26, 0, random(0, 360), 2, random(50, 130));



	//burned demon
	burned_demon:
		TNT1 A 0 A_Playsound("props/redfire");
		tnt1 a 0
		{
			switch( random(0,1) )
			{
				case 0 : sprite = getspriteindex("carb"); frame = 9; break;
				case 1 : sprite = getspriteindex("P1N7"); frame = 4; break;
			}
		}
		"####" "############################################################################" 2
		{
			a_spawnprojectile("FlameTrails", 25, 0, random(0, 360), 2, random(50, 130));
			a_spawnprojectile("BurnParticles", 32, 0, random(0, 360), 2, random(50, 130));
		}
		"####" "############################################################################" 2
		{
			tics = random(15, 25);
			a_spawnprojectile("Explosion_Smoke", 35, 0, random(0, 360), 2, random(70, 110));
		}
		"####" "#" -1;
	Stop;
}
}
























CLASS PinkyHeadPiece : pb_blood_and_gore_base { 
	Default {
	Radius 2;
	Height 2;
	Speed 6;
	Mass 1;
	Decal "BrutalBloodSplat";
	+CLIENTSIDEONLY;
	BounceFactor 0.4;
	+DOOMBOUNCE;
	+NOBLOCKMAP;
	+MISSILE;
	+NOTELEPORT;
	+DONTSPLASH;
	+MOVEWITHSECTOR;
	+FLOORCLIP;
	Scale 1.2;
	}

		XFAT GH 5;
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 AAAAAAAAAAAAAAAAAA 4 a_spawnprojectile("Brutal_LiquidBlood2", 40, 0, random(0, 360), 2, random(30, 50));

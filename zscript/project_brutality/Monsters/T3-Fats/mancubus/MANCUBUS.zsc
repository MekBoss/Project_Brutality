CLASS pb_mancy_bus : proto_fat //Replaces Fatso
{

Default
{
	//SpawnID 1700;
	Scale 1.2;
	MaxStepHeight 16;
	MaxDropOffHeight 24;
	damagefactor "Crush", 10.0;
	damagefactor "Head", 0.65;
	damagefactor "Avoid", 0.0;
	damagefactor "killme", 0.0;
	PainChance "Kick", 255;
	PainChance "ExtremePunches", 255;
	PainChance "Taunt", 255;
	PainChance "Explosive", 255;
	damagefactor "TeleportRemover", 0.0;
	damagefactor "GibRemoving", 0.0;
	DamageFactor "CauseObjectsToSplash", 0.0;
	PainChance "SSG", 255;
	PainChance "Stun", 255;
	Radius 48;
	GibHealth 30;
	Health 650;
	Mass 500;
	BloodType "pb_blood", "BloodSPlatterReplacer", "AxeBlood";
	Obituary "%o was incinerated by a Mancubus.";
	damagefactor "Blood", 0.0; damagefactor "BlueBlood", 0.0; damagefactor "GreenBlood", 0.0;
	+THRUGHOST;
	+BOSSDEATH;
	Species "BigFatAss";
	+DONTHARMSPECIES;
	+DONTHARMCLASS;
	
	//DropItem "DemonStrengthRune", 5;
	//DropItem "Demonpickup2", 60;
	//DropItem "Demonpickup2", 250;
}
States
{

	Spawn:
		TNT1 A 0;
 	Stand:
		FATT B 10 A_Look();
	Loop;
		
	See:
	LookingForPlayer:
		TNT1 A 0 A_PlaySound("knight/step",5);
		tnt1 a 0 {pb_monster_memory(); cooldown --;}
		FATT AA 4 A_Chase();
		TNT1 A 0 A_PlaySound("knight/step",5);
		FATT BBCC 4 A_Chase();
		TNT1 A 0 A_PlaySound("knight/step",5);
		FATT DDEEFF 4 A_Chase();
	Goto See;

	Missile:
		TNT1 A 0 A_JumpIfCloser(900, 2);
		TNT1 A 0 A_JumpIfCloser(300, "Flamethrower");
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, 1);
		FATT G 2 A_FaceTarget();
		FATT GG 10 A_FatRaise();
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,-25,10,0);
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,25,0,0);
		FATT H 10 BRIGHT;
		FATT IG 5 A_FaceTarget();
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,-25,0,0);
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,25,-10,0);
		FATT H 10 BRIGHT;
		FATT IG 5 A_FaceTarget();
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,-25,4,0);
		TNT1 A 0 a_spawnprojectile("BigFireBall",30,25,-4,0);
		FATT H 10 BRIGHT;
		FATT IG 5;
	Goto See;
		
	Flamethrower:
		TNT1 A 0 A_JumpIfInventory("Enraged", 1, 1);
		FATT G 1 A_FatRaise();
		TNT1 A 0 A_PlaySound("FLMDRAW",3);	
		FATT GGGGGGGGGGGG 2
		{
			A_FaceTarget();
			pb_effect_event(effects).show_some_effect(self, "ExplosionParticle");
			pb_effect_event(effects).show_some_effect(self, "ExplosionParticle");
		}
	Torch:
		FATT GHIGH 1
		{
			A_FaceTarget();
			A_PlaySound("flamersh",2);
			if( (frame % 2) == 0) cooldown ++;
			a_spawnprojectile("EnemyFlamethrowerMissile",30,-25,10,0);
			a_spawnprojectile("EnemyFlamethrowerMissile",30,25,0,0);
			pb_effect_event(effects).show_some_effect(self, "ExplosionParticle");
			pb_effect_event(effects).show_some_effect(self, "ExplosionParticle");
		}
		TNT1 A 0 A_JumpIf(cooldown > 8, "See");
		TNT1 A 0 A_JumpIfCloser(320, "Torch");
		FATT IG 5;
	Goto See;
		
	Pain:
		FATT J 3;
		FATT J 3 A_Pain();
	Goto See;
		
	Possession:
		TNT1 A 0 A_SetInvulnerable();
		FATT J 3;
		FATT J 3 A_Pain();
 		TNT1 A 0 A_UnSetInvulnerable();
	Goto See;
		
	Pain.Kick:
		TNT1 A 0 A_FaceTarget();
		TNT1 A 0 A_Recoil(0.50);
		FATT J 9 A_Pain();
		FATT J 8;
	Goto See;
		
	Pain.Explosive:
	Pain.ExtremePunches:
	Pain.SSG:
	Pain.Blackhole:
	Pain.QuadShot:
		TNT1 A 0 A_FaceTarget();
		TNT1 A 0 {self.bNOPAIN = true;}
		FAT4 A 4 ThrustThingZ(0,30,0,1);
		FAT4 B 4 A_Pain();
		FAT4 CDDDD 4;
		FAT4 C 4 A_Stop();
		FAT4 BA 4;
		TNT1 A 0 {self.bNOPAIN = false;}
	Goto See;
		
	Death.Fatality:
		TNT1 A 0 A_Jump(256, "Fatality1", "Fatality2");
		TNT1 A 0;
	Stop;
		
	Fatality1:
		//TNT1 A 0 A_GiveToTarget("GoFatality", 1);
		//TNT1 A 0 A_GiveToTarget("FatsoFatality", 1);
		TNT1 A 1 A_BossDeath();
		TNT1 A 0;
	Stop;
		
	Fatality2:
		//TNT1 A 0 A_GiveToTarget("GoFatality", 1);
		//TNT1 A 0 A_GiveToTarget("FatsoFatality2", 1);
		TNT1 A 1 A_BossDeath();
		TNT1 A 0;
	Stop;

	Death.minorhead:
	Death.head:
		//TNT1 A 0 A_JumpIfInTargetInventory("HasPlasmaWeapon", 1, "Death.Plasma");
		TNT1 A 0 A_SpawnItem("BloodSplasher2");
		TNT1 A 0 A_SpawnItem("MancubusHeadExplode", 0, 50);
		TNT1 A 0 A_Jump(32, "DeathHead2");
		FATT KKKLLL 2 a_spawnprojectile("Brutal_LiquidBlood2", 50, 0, random(0, 360), 2, random(30, 50));
		TNT1 A 0 A_Scream();
		TNT1 A 0 A_NoBlocking();
		TNT1 A 0 A_BossDeath();
		FATT MMMNNOOPPQQRRSSTTTTTTTTTTTTTTTTTTTTTTT 2 a_spawnprojectile("Brutal_LiquidBlood2", 30, 0, random(0, 360), 2, random(20, 50));
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 A 0 A_SpawnItem("DeadFatNoHead", 1);
	Stop;
		
	DeathHead2:
		FATR K 6 A_Scream();
		FATR L 6;
		FATR M 6;
		FATR NOPQR 6;
		FATR S 6 A_NoBlocking();
		FATR T 6 A_BossDeath();
		TNT1 A 0 A_SpawnItem("Deadfat3", 1);
	Stop;
		
		
	Death.decaptate:
		TNT1 AAAAAA 0 a_spawnprojectile("Xdeath1", 62, 0, random(50, 130), 2, random(50, 130));
		TNT1 A 0 a_spawnprojectile("XdeathMancubusHead", 62, 0, random(50, 130), 2, random(50, 130));
		FATT O 6 A_Scream();
		FATT P 6 A_NoBlocking();
		FATT QRS 6;
		FATT T 1 A_BossDeath();
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 A 0 A_SpawnItem("DeadFatNoHead", 1);
	Stop;
		
	Death:
		TNT1 A 0 A_Jump(32, "Death.Arm");
		TNT1 A 0 A_Jump(112, "Death2");
		TNT1 A 0 A_Jump(128, "Death3");
		FATK A 6;
		FATK B 6 A_Scream();
		FATK C 6 A_NoBlocking();
		FATK DEF 6  {pb_blood_and_gore_event.much_blood(self);}
		FATK F 1 A_BossDeath  ();
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 A 0 A_SpawnItem("DeadFat", 1);
	Stop;
		
	Death2:
		FAT3 K 6;
		FAT3 L 6 A_Scream();
		FAT3 M 6 A_NoBlocking();
		FAT3 NOP 6  {pb_blood_and_gore_event.much_blood(self);}
		FAT3 Q 6;
		FAT3 R 6 A_BossDeath();
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 A 0 A_SpawnItem("DeadFat2", 1);
	Stop;
		
	Death3:
		FAT3 A 1;
		FAT3 B 6 A_Scream();
		FAT3 C 6 A_NoBlocking();
		FAT3 DEF 6;
		FAT3  F 1 A_BossDeath();
		FAT3 F 1 A_SpawnItem("DeadFattyHoles", 1);
	Stop;
		
		////////////////////////////////
		
		
		
	Death.Tear:
		TNT1 O 0 A_Jump(128, "DeathGutted");
		TNT1 AAA 0  {pb_blood_and_gore_event.much_blood(self);}
		TNT1 A 0 A_Scream();
		TNT1 O 0 A_NoBlocking();
		FAT2 KL 5  {pb_blood_and_gore_event.much_blood(self);}
		TNT1 AAAA 0 a_spawnprojectile("RipGuts", 0, 0, random(0, 360), 2, random(0, 160));
		TNT1 AA 0 a_spawnprojectile("XDeath1", 0, 0, random(0, 360), 2, random(0, 160));
		TNT1 A 0 A_BossDeath();
		FAT2 MNOP 5  {pb_blood_and_gore_event.much_blood(self);}
		TNT1 A 0 A_SpawnItem("GrowingBloodPool");
		TNT1 A 0 A_SpawnItem("NoGutsFatso", 1);
	Stop;
		
	Death.Cut:
	Death.Saw:
		TNT1 O 0 A_Jump(24, "DeathGutted");
		TNT1 A 0;
		TNT1 A 0 A_Stop();
		XFAT AB 4;
		TNT1 A 0 A_Scream();
		TNT1 O 0 A_NoBlocking();
		TNT1 AA 0  {pb_blood_and_gore_event.much_blood(self);}
		TNT1 AAAA 0 a_spawnprojectile("RipGuts", 40, 0, random(0, 360), 2, random(0, 160));
		TNT1 AAA 0 a_spawnprojectile("XDeath1", 40, 0, random(0, 360), 2, random(0, 160));
		TNT1 AA 0 a_spawnprojectile("XDeath2", 40, 0, random(0, 360), 2, random(0, 160));
		TNT1 AA 0 a_spawnprojectile("XDeath3", 40, 0, random(0, 360), 2, random(0, 160));

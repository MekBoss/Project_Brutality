}

States
{
	Spawn:
		TNT1 A 0 nodelay A_Playsound("BGFLMBL", 6, 1, 1);
	Spawn1:
		BLHD CD 1 BRIGHT
		{
			 A_SpawnItem("CoolandNewFlameTrails");
			a_spawnprojectile("ExplosionSmoke", 2, 0, random(70, 110), 2, random(0, 360));
		}
	Loop;
	
	death:
		TNT1 A 0 {self.bNOGRAVITY = false;}
		TNT1 A 0 A_Explode(10, 50, 0);
		TNT1 A 0;
		TNT1 A 0 A_StopSound(6);
		TNT1 AAAA 0 A_SpawnItemEx("ExplosionFlareSpawner",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		TNT1 A 0
		{
			for (int i = 0; i < 10; i++)
			{
				a_spawnprojectile("pb_particle", 0, 0, random(0, 360), 2, random(0, 180));
			}
		}
		TNT1 AAA 0 
		{
			pb_effect_event(effects).show_some_effect(self, "ExplosionParticle");
		}
		EXPL AAA 0 a_spawnprojectile("FlamethrowerFireParticles", 6, 0, random(0, 360), 2, random(0, 90));
		//TNT1 AA 0 a_spawnprojectile("FireworkSFXType1", 0, 0, random(0, 360), 2, random(30, 60));
		TNT1 A 0 A_SpawnItemEx("TinyBurningPiece", random(-15, 15), random(-15, 15));
		TNT1 A 0 A_SpawnItemEx("TinyBurningPiece", random(-35, 35), random(-35, 35));
		TNT1 A 0 A_SpawnItemEx("TinyBurningPiece", random(-45, 45), random(-45, 35));
		TNT1 A 1 Light("BIGREDTORCH");
		TNT1 AAAAAA 20 a_spawnprojectile("BigBlackSmoke", 0, 0, random(0, 360), 2, random(40, 160));
	Stop;
}

}



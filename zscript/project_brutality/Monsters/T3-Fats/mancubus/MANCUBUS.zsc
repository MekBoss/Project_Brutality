CLASS BigFireBall  : pb_projectile_base //Replaces Fatshot
{ 
Default
{	
	//SpawnID 153;
	Radius 6;
	Height 8;
	Speed 20;
	Fastspeed 25;
	DamageFunction (random(25,40));
	Projectile;
	Scale 0.71;
	+FORCEXYBILLBOARD;
	+THRUGHOST;
	RenderStyle "Translucent";
	DamageType "Fire";
	ExplosionRadius 150;
	ExplosionDamage 8;
	Alpha 1;
	Decal "Scorch";
	SeeSound "BREATHOF";

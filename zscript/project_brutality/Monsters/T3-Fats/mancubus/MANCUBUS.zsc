	Crush:
		TNT1 A 0 A_PlaySound("misc/xdeath4");
		CRS3 A 5 A_BossDeath();
		CRS3 A -1;
	Stop;
	
	Pain.Taunt:
		tnt1 B 0
		{
			A_Pain();
		} 

		TROT GG 2 a_spawnprojectile("ImpAttack2",10,0,0,0);
		TROT G 1;
		//TNT1 A 0 A_JumpIfInventory("Fatality_Marine", 1, "FatalityMarine");
		//TROT HHH 2 A_JumpIfInventory("Fatality_Marine", 1, "FatalityMarine");

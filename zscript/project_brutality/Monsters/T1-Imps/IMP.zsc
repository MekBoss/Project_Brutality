CLASS pb_imp_fireball : pb_projectile_base//DoomImpBall// Replaces DoomImpBall : ACTOR
{ 
Default
{
	DamageType "Burn";
	Radius 8;
	Height 4;
	Speed 16;
	FastSpeed 20;
	DamageFunction (random(15,20));
	PROJECTILE;
	+THRUGHOST;
	RenderStyle "Add";
	Alpha 1;
	SeeSound "ImpFire";
	DeathSound "ImpFireImpact";
	Decal "Scorch";
	Scale 0.25;
}
States
{
	Spawn:
	TNT1 A 0;
	TNT1 A 0 A_JumpIf(waterlevel > 1, "SpawnUnderwater");
	TNT1 A 0;//A_JumpIf(ACS_NamedExecuteWithResult("PerformanceMode")==1,"SpawnCheap")
	//TNT1 A 0 ACS_ExecuteAlways(798, 0, 0, 0, 0);//Check Effects
	Spawn1:
	TNT1 A 0 A_JumpIfInventory("lowgraphicsmode", 1, "SpawnCheap");
	TNT1 A 0 A_Playsound("SMLFRBL", 1, 1, 1);
	Fly:
	TNT1 A 0 A_SpawnItem ("CoolAndNewFlameTrails", 0, random( -2, 2));
	BLHD C 1 BRIGHT A_SpawnItem("FireballFlare",2,0);
	TNT1 A 0 a_spawnprojectile ("OldschoolRocketSmokeTrail2", 2, 0, random (160, 210), 2, random (-30, 30));
	TNT1 A 0 A_SpawnItem ("CoolAndNewFlameTrails");
	BLHD D 1 BRIGHT A_SpawnItem("FireballFlare",0,0);
	Loop;

	SpawnCheap:
	TNT1 A 0;//A_SpawnItem ("CoolAndNewFlameTrails")
	BLHD A 1 BRIGHT A_SpawnItem("FireballFlare",0,0);
	TNT1 A 0;//A_SpawnItem ("CoolAndNewFlameTrails")
	BLHD B 1 BRIGHT A_SpawnItem("FireballFlare",0,0);
	Loop;
	
	SpawnUnderwater:
	BLHD A 1 BRIGHT A_SpawnItem("FireballFlare",0,0);
	TNT1 A 0 a_spawnprojectile ("BUBULZ", 0, 0, random (0, 360), 2, random (0, 180));
	BLHD B 1 BRIGHT A_SpawnItem("FireballFlare",0,0);
	TNT1 A 0 a_spawnprojectile ("BUBULZ", 0, 0, random (0, 360), 2, random (0, 180));
	Goto Spawn1;
	
	Death:
		TNT1 A 0 {self.bSHOOTABLE = false;}
		TNT1 A 0 A_StopSound(1);
		tnt1 a 0 a_spawnitem("ShrapnelParticle");
		TNT1 A 0 A_JumpIfInventory("lowgraphicsmode", 1, "DeathCheap");
		TNT1 A 0 A_SpawnItemEx ("SmallUnderwaterExplosion",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		EXPL AAAAAA 0 a_spawnprojectile ("FireBallExplosionFlames", 1, 0, random (0, 360), 2, random (0, 360));
		//FRFX CDEFGHIJKLMNOP 1 BRIGHT A_SpawnItem("RedFlare",0,0)
		TNT1 A 0 A_SpawnItemEx("TinyBurningPiece3", random (-5, 5), random (-5, 5));
		TNT1 A 0 A_SpawnItemEx ("DetectFloorCraterSmall",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		TNT1 A 0 A_SpawnItemEx ("DetectCeilCraterSmall",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		TNT1 AAAAAAAAAA 0 a_spawnprojectile ("ExplosionParticle", 0, 0, random (0, 180), 2, random (0, 180));
		TNT1 AAAA 19 a_spawnprojectile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 160));
		Stop;
		XDeath:
		TNT1 A 0 A_StopSound(1);
		TNT1 A 0 {self.bSHOOTABLE = false;}
		tnt1 a 0 a_spawnitem("ShrapnelParticle");
		TNT1 A 0 A_JumpIfInventory("lowgraphicsmode", 1, "DeathCheap");
		TNT1 AAAAAAAAAA 0 a_spawnprojectile ("ExplosionParticle", 0, 0, random (0, 180), 2, random (0, 180));
		TNT1 A 0 A_SpawnItemEx ("SmallUnderwaterExplosion",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		EXPL AAAAAA 0 a_spawnprojectile ("FireBallExplosionFlames", 1, 0, random (0, 360), 2, random (0, 360));
		EXPL AAA 0 a_spawnprojectile ("PlasmaSmoke", 1, 0, random (0, 360), 2, random (0, 360));
		TNT1 A 0 A_SpawnItemEx("TinyBurningPiece3", random (-15, 15), random (-15, 15));
		//FRFX CDEFGHIJKLMNOP 1 BRIGHT A_SpawnItem("RedFlare",0,0)
	Stop;

	DeathCheap:
		TNT1 A 0 A_StopSound(1);
		TNT1 A 0 {self.bSHOOTABLE = false;}
		TNT1 AAA 0 a_spawnprojectile ("ExplosionParticle", 0, 0, random (0, 180), 2, random (0, 180));
		TNT1 A 0 A_SpawnItemEx ("SmallUnderwaterExplosion",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0);
		EXPL AAAA 0 a_spawnprojectile ("FireBallExplosionFlames", 1, 0, random (0, 360), 2, random (0, 360));
	Stop;
}

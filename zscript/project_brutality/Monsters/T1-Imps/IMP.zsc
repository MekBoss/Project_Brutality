	Pain:
		TNT1 A 0;
		TNT1 A 0 {self.bNOGRAVITY = false; self.bTHRUSPECIES = false; self.bCEILINGHUGGER = false;}
		TNT1 A 0 { if( self.on_ceiling == true) { setstatelabel("PainCanFallFromCeiling");} }

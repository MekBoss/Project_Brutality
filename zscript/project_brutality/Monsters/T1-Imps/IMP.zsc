		//TNT1 A 0 A_takeInventory("SKImp", 1);
		TNT1 A 0 A_JumpIfCloser(1200, 3);
		//TNT1 A 0 a_spawnprojectile("MonsterTargetCheck", 34, 0, 0, 0);

		TNT1 A 0 a_spawnprojectile ("SmallFlameTrails", 25, -30, random (0, 40), 2, random (0, 160));
		TNT1 A 0 a_spawnprojectile ("ImBallGettingReady", 25, -30, random (0, 140), 2, random (0, 160));

	+DONTHARMSPECIES;
	+DONTHARMCLASS;
}
States 
{ 
	Death.Blackhole:
		TNT1 A 0 A_Scream;
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0 A_SpawnItem("BlackHoledMagcaco");
	Stop;
		
	Death.Fatality:
		TNT1 A 0 A_Pain;
		//TNT1 A 0 A_JumpIfIntargetInventory("FistsSelected", 1, 1);
		Goto Death;
		TNT1 A 0 A_NoBlocking;
		//TNT1 A 0 A_GiveToTarget("GoFatality", 1);
		//TNT1 A 0 A_GiveToTarget("MagCacoFatality", 1);
	Stop;
		
	Spawn: 
		//TNT1 A 0 ACS_ExecuteAlways(796, 0, 0, 0, 0);//Check if Vanilla Mode is activated
		TNT1 AA 0;
	watch:
		INFR A 8 Bright A_Look;
	loop;

	See: 
		INFR BCD 2 Bright A_Chase;
		Loop;
	Missile:
		TNT1 A 0 A_JumpIfCloser(250, "Torch");
		TNT1 A 0 A_Jump(16, "WatcherComets");
	Fireballs:
		INFR HI 4 Bright A_FaceTarget;
		INFR J 0 Bright a_spawnprojectile ("FireShot",24,0,0,0,0);
		INFR J 4 BRIGHT;
		Goto See;
	WatcherComets:
		TNT1 A 0 A_JumpIfCloser(600, "Fireballs");
		INFR HI 4 Bright A_FaceTarget;
		INFR J 0 Bright a_spawnprojectile ("WatcherSummonComet",24,0,0,0,0);
		INFR J 4 BRIGHT;
		Goto See;
	Torch:
		INFR HI 4 Bright A_FaceTarget;
		TNT1 A 0 A_PlaySound("flamersh",2);
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR J 2 BRIGHT;
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR I 2 BRIGHT;
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR J 2 BRIGHT;
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR I 2 BRIGHT;
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR J 2 BRIGHT;
		INFR J 0 Bright a_spawnprojectile ("EnemyFlamethrowerMissile",24,0,0,0,0);
		INFR I 2 BRIGHT;
	Goto See;

	Melee:
		TNT1 A 0 A_PlaySound("monster/ovlbit");
		INFR AFG 5 A_Facetarget;
		TNT1 A 0 a_spawnprojectile("MagCacodemonBite", 34, 0, 0, 0);
		INFR E 4 A_Recoil(-4);
		Goto See;
		
	Pain: 
		INFR K 3 Bright A_Pain;
		INFR K 3 Bright;
		Goto See;
		
	Possession:
		TNT1 A 0 A_SetInvulnerable;
		INFR K 3;
		INFR K 3 A_Pain;
		"####" "#" 35;// ACS_NamedExecuteAlways("Pos - Flicker effect");
		TNT1 A 0 A_UnSetInvulnerable;
		Goto See;
		
	Pain.Stun:
		TNT1 A 0;
		TNT1 A 0 A_SpawnItemEx ("StunElectrocute", random (-4, 4), random (-4, 4),  random (16, 32), 0, 0);
		INFR K 1 A_Pain;
		INFR KKKKKKKKKK 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
		INFR K 1 A_Pain;
		INFR KKKKKKKKKK 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
		INFR K 1 A_Pain;
		INFR KKKKKKKKKK 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
		INFR K 1 A_Pain;
		INFR KKKKKKKKKK 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
		INFR K 1 A_Pain;
		INFR KKKKKKKKKK 3 A_SpawnItemEx ("StunElectrocute", random (-12, 12), random (-12, 12),  random (16, 52), 0, 0);
		INFR K 1 A_Pain;
		Goto See;
		
	Pain.Kick:
		TNT1 A 0 A_FaceTarget;
		INFR K 6 A_Recoil(5);
		INFR K 6 A_Pain;
		INFR K 10;
		Goto See;
		
	Death: 
		INFR L 4 Bright;
		INFR M 3 Bright A_Scream;
		INFR N 3 Bright;
		TNT1 A 0 A_Explode (20,100);
		TNT1 AAAAAA 0 bright a_spawnprojectile ("SuperGoreSpawner", 5, 0, random (0, 360), 2, random (30, 180));
		TNT1 AAAAAAAAA 0 a_spawnprojectile ("CeilBloodLauncherLong", 0, 0, random (0, 360), 2, random (50, 130));
		TNT1 A 0 a_spawnprojectile ("XDeathBrain1", 32, 0, random (0, 360), 2, random (0, 160));
		TNT1 A 0 a_spawnprojectile ("XDeathBrain2", 32, 0, random (0, 360), 2, random (0, 160));
		//TNT1 AAAAAAAA 0 a_spawnprojectile ("PEPart1", 32, 0, random (0, 360), 2, random (0, 160));

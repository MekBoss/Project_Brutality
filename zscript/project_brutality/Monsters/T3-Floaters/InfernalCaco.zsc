	damagefactor "killme", 0.0;
	
	DropItem "DemonStrengthRune", 5;
	DropItem "DemonStrengthRune", 3;
	DropItem "RageSphere", 4;
	DropItem "DoubleSphere", 2;
	DropItem "ShieldSphere", 2;
	DropItem "Demonpickup2", 65;
	DropItem "Demonpickup", 255;
	

CLASS ZombieXDeath : ACTOR { 
	Default {
		
		Radius 1;
		Height 1;
		Scale 0.8;
		Speed 0;
		Mass 1;
		Decal BloodSuper;
		BounceFactor 0.5;
		+DOOMBOUNCE;
		+MISSILE;
		+NOBLOCKMAP;
		+NOTELEPORT;
		+MOVEWITHSECTOR;
		+NOGRAVITY;
		+NOCLIP;
		+DONTSPLASH;
	}
	States
	{
		Spawn:
			TNT1 A 0;
			TNT1 A 0 A_Pain();
			TNT1 A 0 A_FaceTarget();
			TNT1 A 0 A_SpawnItem("BloodSplasher3");
			
			TNT1 AAAAAAAAAAAAAAA 0 a_spawnprojectile ("SuperWallRedBlood", 40, 0, random (0, 360), 2, random (-15, 15));
			TNT1 AAAAAAAAA 0 a_spawnprojectile ("CeilBloodLauncherLong", 0, 0, random (0, 360), 2, random (50, 130));
			TNT1 A 0 a_spawnprojectile ("XDeathArm1", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 AA 0 a_spawnprojectile ("XDeath1", 32, 0, random (0, 360), 2, random (0, 160));
			//TNT1 A 0 a_spawnprojectile ("XDeath2", 32, 0, random (0, 360), 2, random (0, 160))
			//TNT1 A 0 a_spawnprojectile ("XDeath3", 32, 0, random (0, 360), 2, random (0, 160))
			TNT1 A 0 a_spawnprojectile ("XDeath4", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeath4b", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeathHalfZombiemanNoArmNoHead", 48, 0, random (0, 360), 2, random (0, 160));
			//TNT1 A 0 a_spawnprojectile ("XDeath5", 32, 0, random (0, 360), 2, random (0, 160))
			TNT1 AAAA 0 a_spawnprojectile ("XDeath2b", 15, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAA 0 a_spawnprojectile ("XDeath3b", 15, 0, random (0, 360), 2, random (0, 160));
			TNT1 AA 0 a_spawnprojectile ("XDeathSkinPiece2", 15, 0, random (0, 360), 2, random (0, 160));
			TNT1 AA 0 a_spawnprojectile ("XDeathSkinPiece1", 15, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeathOrgan1", 24, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeathOrgan2", 24, 0, random (0, 360), 2, random (0, 160));
			TNT1 AAAAAAA 0 a_spawnprojectile ("Instestin", 24, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeathZombieLeg", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("GibZombieBoot", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 a_spawnprojectile ("XDeathZombiemanHeadOnXDeath", 32, 0, random (0, 360), 2, random (0, 160));
			TNT1 A 0 A_SpawnItem ("BigBloodSpot");
			
			TNT1 A 1;
			TNT1 A 0 ACS_NamedExecuteAlways("DetermineUserBloodAmount", 0, 0, 0, 0);//Check Blood density
			TNT1 A 2;
			TNT1 AAAAAAAAAAAAAA 0 a_spawnprojectile ("BloodMistExtraBig", 30, 0, random (0, 360), 2, random (20, 90));
			Stop;
			
		LowBlood:
			TNT1 A 2;
			Stop;
		LotsOfBlood:
			TNT1 AAAAAAAAAAAAAA 0 a_spawnprojectile ("BloodMistExtraBig", 30, 0, random (0, 360), 2, random (20, 90));
			TNT1 AAAAAA 0 a_spawnprojectile ("SuperGoreSpawner2", 64, 0, random (0, 360), 2, random (20, 40));
			Stop;
		AbsurdBlood:
			TNT1 AAAAAAAAAAAAAA 0 a_spawnprojectile ("BloodMistExtraBig", 30, 0, random (0, 360), 2, random (20, 90));
			TNT1 AAAAAAAAAAAA 0 a_spawnprojectile ("SuperGoreMist", 24, 0, random (0, 360), 2, random (30, 90));
			TNT1 AAAAAAAA 0 a_spawnprojectile ("SuperGoreSpawner2", 64, 0, random (0, 360), 2, random (20, 40));
			Stop;
	}
}


CLASS WaterParticleXSpawner : ACTOR { 
Default {
	
	Projectile;
	+RANDOMIZE;
	+FORCEXYBILLBOARD;
	//+NOINTERACTION
	// +NOGRAVITY
	+THRUACTORS;
	+NODAMAGETHRUST;
	Mass 9999999;
	damage 0;
	radius 2;
	height 0;
	speed 40;
	renderstyle "Add";
	alpha 0.9;
	scale .15;
}
states
{
	Spawn:
		TNT1 AAAA 1 A_JumpIf(waterlevel < 1, "SurfaceSpawn");
		Stop;
		
	SurfaceSpawn:
		TNT1 AAAA 0 A_SpawnItem("SpawnLiquidParticleX");
		TNT1 AA 1 A_SpawnItem("SpawnLiquidParticleX");
		TNT1 A 16;
		TNT1 A 0;//A_SpawnItem("WaterSplashWaves")
		TNT1 A 16;
		TNT1 A 0 {self.bSHOOTABLE = false;}
		TNT1 A 0 A_NoBlocking;
		TNT1 A 0;//A_SpawnItem("WaterSplashWaves")
		TNT1 A 16;
		TNT1 A 0;//A_SpawnItem("WaterSplashWaves")
		Stop;
		Stop;
	Death:
		Stop;
	XDeath:
		TNT1 A 0;
		Stop;
}
}



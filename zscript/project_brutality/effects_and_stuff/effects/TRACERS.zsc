	TNT1 A 0;
	TNT1 A 0 A_CheckFloor("XDeath");
	TNT1 A 0 A_CheckCeiling("XDeath");
	
	TNT1 A 1;
	TNT1 AAAA 0 A_SpawnItemEx("BluePlasmaParticleSpawner", 0, 0, 0, 0, 0, 0, 0, 128);
